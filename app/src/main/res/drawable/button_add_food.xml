<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:dashGap="10dp"
                android:dashWidth="10dp"
                android:color="@color/colorFoodAddButton" />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
    <item android:gravity="center" android:width="40dp" android:height="40dp">
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:dashGap="10dp"
                android:dashWidth="10dp"
                android:color="@color/colorFoodAddButton" />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
    <item android:gravity="center" android:width="25dp" android:height="3dp">
        <shape android:shape="line">
            <stroke
                android:width="2dp"
                android:color="@color/colorFoodAddButton" />
        </shape>
    </item>
    <item android:gravity="center" android:width="25dp" android:height="3dp">
        <rotate
            android:fromDegrees="90"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="@color/colorFoodAddButton" />
            </shape>

        </rotate>
    </item>
</layer-list>